<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Inherit Form View to Modify it -->
        <record id="sale_order_line_form_inh" model="ir.ui.view">
            <field name="name">sale</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">

                <xpath expr="//page[@name='order_lines']/field[@name='order_line']/tree/field[@name='price_unit']"
                       position="after">
                    <field name="shipping"/>
                </xpath>

                <!--                <xpath expr="//page[@name='order_lines']/field[@name='order_line']/tree/field[@name='product_id']"-->
                <!--                       position="before">-->
                <!--                    <field name="is_new_field"/>-->
                <!--                </xpath>-->

            </field>
        </record>


        <record id="account_move_line_form_inh" model="ir.ui.view">
            <field name="name">account</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">

                <xpath expr="//page[@id='invoice_tab']/field[@name='invoice_line_ids']/tree/field[@name='tax_ids']"
                       position="after">
                    <field name="re_shiping_test"/>
                </xpath>

                <xpath expr="//field[@name='line_ids']/tree/field[@name='account_id']"
                       position="after">
                    <field name="re_shiping_test" invisible="1"/>
                </xpath>

<!--                <xpath expr="//field[@name='tax_totals_json']" position="before">-->
<!--                    <field name="shiping_one"/>-->
<!--                </xpath>-->


                <!--                 <xpath expr="//group[@class='oe_subtotal_footer oe_right']" position="after">-->
                <!--                    <field name="re_shiping"/>-->
                <!--                </xpath>-->

            </field>
        </record>


    </data>
</odoo>